{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "target": "es6",
    "module": "es6",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "noEmitOnError": true,
    "noEmit" :true,
    "allowImportingTsExtensions": true
  },
  "include": ["./*.ts", "./**/*.ts"]
}
